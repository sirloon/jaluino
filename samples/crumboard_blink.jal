--
-- Description: this program blinks the two LEDs available on Crumboard shield
--
-- Shield: crumboard
--

include jaluino_medium
include crumboard_shield
include delay

crumboard_init()

crumboard_led1 = off
crumboard_led2 = off

forever loop
   crumboard_led1 = on
   delay_1ms(250)
   crumboard_led1 = off

   crumboard_led2 = on
   delay_1ms(250)
   crumboard_led2 = off
end loop

