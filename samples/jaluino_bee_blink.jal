--
-- Description: this program blinks a LED for ever... LED is built-in, onboard, connected
-- to pin RC2
--

include jaluino_bee
include delay

-- Assuming LED is enable on hardware (jumper)
onboard_led_direction =  output

forever loop
   onboard_led = on
   delay_1ms(250)
   onboard_led = off
   delay_1ms(250)
end loop

