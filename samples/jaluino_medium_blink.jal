--
-- Description: this program blinks a LED for ever... LED is connected to RA0, through a 1K 
-- resistor
--

include jaluino_medium
include delay

pin_A0_direction =  output
alias   led      is pin_A0

forever loop
   led = on
   delay_1ms(250)
   led = off
   delay_1ms(250)
end loop

